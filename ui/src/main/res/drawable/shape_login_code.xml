<?xml version="1.0" encoding="utf-8"?>


<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:bottomRightRadius="@dimen/dp_30" android:topRightRadius="@dimen/dp_30" />
            <stroke android:width="@dimen/dp_0_5" android:color="@color/colorLine"></stroke>
            <solid android:color="@color/colorCode" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:bottomRightRadius="@dimen/dp_30" android:topRightRadius="@dimen/dp_30" />
            <stroke android:width="@dimen/dp_0_5" android:color="@color/colorLine"></stroke>
            <solid android:color="@color/color_enable" />
        </shape>
    </item>

</selector>